<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

    <record id="payment_acquirer_request_quotation" model="payment.acquirer">
      <field name="name">Request Quotation</field>
      <field name="provider">transfer</field>
      <field name="company_id" ref="base.main_company"/>
      <field name="view_template_id" ref="request_quotation_form"/>
      <field name="environment">test</field>
      <field name="website_published" eval="True"/>
      <field name="pre_msg"><![CDATA[
		  <p>Request Quotation information will be provided after choosing the payment aquirer.</p>]]>
      </field>
      <field name="pending_msg"><![CDATA[
		  <span><i>Pending,</i> your Quotation Request is received, and we will contact you soon.</span>]]>
      </field>
      <field name="post_msg"><![CDATA[
		  <p>Thank you for your request!</p>]]>
      </field>
    </record>
  </data>
</odoo>
